{
  "plugins": ["katex", "include-codeblock"],
  "pluginsConfig": {
    "katex": {
      "delimiters": [
        { "left": "$$", "right": "$$", "display": false },
        { "left": "$", "right": "$", "display": false }
      ]
    },
    "include-codeblock": {
      "fixlang": true,
      "unindent": true
    }
  },
  "styles": {
    "website": "./styles.css"
  }
}
