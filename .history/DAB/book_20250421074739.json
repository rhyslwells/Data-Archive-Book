{
  "plugins": ["katex", "include-codeblock"],
  "pluginsConfig": {
    "katex": {
      "delimiters": [
        { "left": "$$", "right": "$$", "display": true },
        { "left": "$", "right": "$", "display": false }
      ]
    },
    "include-codeblock": {
      "fixlang": true,
      "unindent": true
    }
  }
}
